<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jacksondidat.memesoundboard.MainSoundboard"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/yes"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:text="To Be Continued"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toRightOf="@+id/seinfeld"
        android:layout_marginLeft="18dp" />

    <Button
        android:id="@+id/seinfeld"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginStart="8dp"
        android:text="Seinfeld Bass"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/jazz"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginEnd="7dp"
        android:text="Ya Like Jazz?"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/yes"
        android:layout_marginLeft="18dp"
        app:layout_constraintHorizontal_bias="1.0" />

    <Button
        android:id="@+id/ah"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:text="AHHHHH"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/jazz"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/lobster"
        android:layout_marginLeft="18dp"
        app:layout_constraintHorizontal_bias="1.0" />

    <Button
        android:id="@+id/lobster"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:text="Rock Lobster"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/yes"
        app:layout_constraintLeft_toRightOf="@+id/seal"
        android:layout_marginLeft="18dp" />

    <Button
        android:id="@+id/seal"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:text="Sax Seal"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/seinfeld" />

    <Button
        android:id="@+id/swamp"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Get Out of My Swamp!"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seal" />

    <Button
        android:id="@+id/all_star"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="8dp"
        android:text="All Star"
        app:layout_constraintLeft_toRightOf="@+id/swamp"
        app:layout_constraintTop_toBottomOf="@+id/lobster" />

    <Button
        android:id="@+id/sax_guy"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginLeft="19dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Epic Sax Guy"
        app:layout_constraintLeft_toRightOf="@+id/all_star"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ah" />

    <Button
        android:id="@+id/crawling"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginLeft="19dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Crawling in My Skin"
        app:layout_constraintLeft_toRightOf="@+id/wake"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sax_guy" />

    <Button
        android:id="@+id/wake"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginTop="8dp"
        android:text="Wake Me Up Inside"
        app:layout_constraintTop_toBottomOf="@+id/all_star"
        app:layout_constraintLeft_toRightOf="@+id/run"
        android:layout_marginLeft="17dp" />

    <Button
        android:id="@+id/run"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Run"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/swamp" />

    <Button
        android:id="@+id/suh"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Suh dude"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/run" />

    <Button
        android:id="@+id/cut"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="8dp"
        android:text="Cut My Life into Pieces"
        app:layout_constraintLeft_toRightOf="@+id/suh"
        app:layout_constraintTop_toBottomOf="@+id/wake" />

    <Button
        android:id="@+id/photograph"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginLeft="19dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Look at This Photograph"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/cut"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/crawling" />
</android.support.constraint.ConstraintLayout>
