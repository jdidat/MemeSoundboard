<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jacksondidat.memesoundboard.MainSoundboard"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/yes"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginTop="16dp"
        android:text="To Be Continued"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintLeft_toRightOf="@+id/seinfeld"
        android:layout_marginLeft="8dp" />

    <Button
        android:id="@+id/seinfeld"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginLeft="6dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="Seinfeld Bass"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/jazz"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginEnd="7dp"
        android:layout_marginRight="7dp"
        android:layout_marginTop="16dp"
        android:text="Ya Like Jazz?"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/ah"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginLeft="19dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:text="AHHHHH"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintLeft_toRightOf="@+id/lobster"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/jazz" />

    <Button
        android:id="@+id/lobster"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="16dp"
        android:text="Rock Lobster"
        app:layout_constraintLeft_toRightOf="@+id/seal"
        app:layout_constraintTop_toBottomOf="@+id/yes" />

    <Button
        android:id="@+id/seal"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:text="Sax Seal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seinfeld" />
</android.support.constraint.ConstraintLayout>
